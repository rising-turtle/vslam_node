
<launch>
    <arg name="vins_folder" default="$(find vslam_vins)"/>
    <arg name="config_path" default = "$(arg vins_folder)/config/tum_vio_config.yaml" />
    <arg name="vins_path" default = "$(arg vins_folder)/../result/" />
 
    <node pkg="feature_tracker_ext" type="kf_tracker" name="kf_tracker" output="screen" cwd="node">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="vins_folder" type="string" value="$(arg vins_path)" />
	<remap from="/kf_tracker/feature_img" to="/feature_tracker_ext/feature_img"/>
    </node>

    <node name="vins_estimator_ext" pkg="vins_estimator_ext" type="vins_estimator_ext" output="screen" cwd="node">
       <remap from="/feature_tracker_ext/feature" to="/kf_tracker/feature"/>
       <param name="config_file" type="string" value="$(arg config_path)" />
       <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

</launch>
